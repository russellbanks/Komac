{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":label(dependencies)",
    ":maintainLockFilesWeekly",
    ":semanticCommitsDisabled"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["clap", "clap_complete"],
      "groupName": "clap ecosystem"
    },
    {
      "matchPackageNames": ["cynic", "cynic-codegen"],
      "groupName": "cynic ecosystem"
    }
  ],
  "rangeStrategy": "bump"
}
